/div>\n                <div class=\"common-bg economics-item\">\n                    <div class=\"icon\">\n                        <img src=\"../../assets/icon-economics-3.png\" alt />\n                    </div>\n                    <div class=\"item-title\">{{ localeData['economics.Staking'] }}</div>\n                </div>\n                <div class=\"common-bg economics-item\">\n                    <div class=\"icon\">\n                        <img src=\"../../assets/icon-economics-4.png\" alt />\n                    </div>\n                    <div class=\"item-title\">{{ localeData['economics.earn'] }}</div>\n                </div>\n            </div>\n        </div>\n        <div class=\"normal-layout section-content supporter\">\n            <div class=\"title\">{{ localeData['supporter.title'] }}</div>\n            <div class=\"friends-list\">\n                <a href target=\"_blank\" class=\"friend-item\">\n                    <img src=\"../../assets/supporter-yooshi.png\" alt=\"\">\n                </a>\n                <a href target=\"_blank\" class=\"friend-item\">\n                    <img src=\"../../assets/supporter-binance.png\" alt=\"\">\n                </a>\n                <a href target=\"_blank\" class=\"friend-item\">\n                    <img src=\"../../assets/supporter-coinmarket.png\" alt=\"\">\n                </a>\n                <a href target=\"_blank\" class=\"friend-item\">\n                    <img src=\"../../assets/supporter-cer.png\" alt=\"\">\n                </a>\n                <a href target=\"_blank\" class=\"friend-item\">\n                    <img src=\"../../assets/supporter-cgc.png\" alt=\"\">\n\n                </a>\n            </div>\n        </div>\n    </section>\n</template>\n\n\n<script setup>\nimport useLocale from '@/hooks/useLocale'\nconst localeData = useLocale()\n</script>\n\n<style lang=\"less\" scoped>\n@import \"./index.less\";\n.section-friends {\n    .economics {\n        padding-top: 120px;\n    }\n    .title {\n        text-align: center;\n    }\n    .economics-list {\n        padding: 60px 0 100px;\n        display: flex;\n        justify-content: center;\n        margin-left: -15px;\n        margin-right: -15px;\n    }\n    .economics-item {\n        flex: 1;\n        margin: 15px;\n        padding: 48px 40px 50px;\n        transition: transform 0.4s ease;\n    }\n    .icon {\n        height: 0;\n        padding-bottom: 100%;\n        position: relative;\n        img {\n            position: absolute;\n            width: 100%;\n            height: 100%;\n            left: 0;\n            top: 0;\n            object-fit: contain;\n        }\n    }\n    .item-title {\n        font-size: 18px;\n        font-weight: 600;\n        text-align: center;\n        margin-top: 30px;\n    }\n}\n.friends-list {\n    padding: 60px 0 100px;\n    display: flex;\n    flex-wrap: wrap;\n    align-items: center;\n    justify-content: center;\n    margin-left: -10px;\n    margin-right: -10px;\n    .friend-item {\n        width: 224px;\n        height: 75px;\n        background-repeat: no-repeat;\n        background-position: 50%;\n        background-size: contain;\n        margin: 10px;\n        filter: grayscale(1);\n        transition: filter 0.4s ease;\n        img{\n            width:100%;\n            height:100%;\n            object-fit: contain;\n        }\n        &:hover{\n            filter: none;\n        }\n    }\n}\n\n@media screen and (max-width: 960px) {\n    .section-friends .economics {\n        padding: 60px 20px 0;\n    }\n    .section-friends .economics-list {\n        padding: 25px 0 50px;\n        flex-wrap: wrap;\n    }\n    .section-friends .economics-list .economics-item {\n        width: 240px;\n        -webkit-box-flex: unset;\n        -ms-flex: unset;\n        flex: unset;\n        margin: 8px;\n    }\n    .section-friends .supporter {\n        padding: 0 20px;\n    }\n    .section-friends .friends-list {\n        padding: 25px 0 50px;\n    }\n    .section-friends .friends-list .friend-item {\n        margin: 8px;\n    }\n}\n@media screen and (max-width: 490px) {\n    .section-friends .economics-list .economics-item {\n        padding: 30px 20px 25px;\n        width: calc(50vw - 26px);\n    }\n    .section-friends .economics-list .economics-item .item-title {\n        font-size: 16px;\n        margin-top: 20px;\n    }\n    .section-friends .friends-list {\n        justify-content: flex-start;\n    }\n    .section-friends .friends-list .friend-item {\n        width: calc(50vw - 26px);\n        height: calc(16.74vw - 8.7048px);\n    }\n}\n</style>"],"sourceRoot":""}]}